            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox IsChecked="{Binding Examine_Part}" Content="检查部位" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox IsChecked="{Binding Device_Number}" Content="设备型号" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox  IsChecked="{Binding Host_Name}" Content="主机名称" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox IsChecked="{Binding Examine_Test}" Content="检查项目" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox IsChecked="{Binding Examine_Method}" Content="检查方法" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox  IsChecked="{Binding Exists_Image}" Content="影像文件" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox  IsChecked="{Binding Image_Quality}" Content="影像质量" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox  IsChecked="{Binding Charge1}" Content="收费金额" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox  IsChecked="{Binding Diagnosis_Doctor}" Content="诊断医师" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
            <Grid Style="{StaticResource ChangeColorGridStyle}" HorizontalAlignment="Left" Width="150" Height="40" MouseLeftButtonDown="Grid_MouseLeftButtonDown"  >
                <CheckBox IsChecked="{Binding IsPositive}"  Content="阳性标识" Height="15" Margin="8,3,2,4">
                    <CheckBox.LayoutTransform>
                        <ScaleTransform ScaleX="1.5" ScaleY="1.5" />
                    </CheckBox.LayoutTransform>
                </CheckBox>
            </Grid>
        </WrapPanel>
    </Grid>
</Window>
